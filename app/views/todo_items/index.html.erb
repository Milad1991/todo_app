<div class="container"
<p id="notice"><%= notice %></p>
<div class="jumbotron">
  <div class="container">

  </div>
</div>
<h1>Busy? This can help you stay organized.</h1>


<%= link_to new_todo_item_path do  %>
  <i class="fa fa-puzzle-piece fa-2x">  Add new task.</i>
  <% end %>
  <tbody>
    <table class="table">
    <% @todo_items.each do |todo_item| %>
      <tr>
        <td><%= todo_item.description %></td>
        <td><%= link_to 'Show', todo_item, class:"btn btn-info pull-right" %></td>
        <td><%= link_to 'Edit', edit_todo_item_path(todo_item), class:"btn btn-info pull-right" %></td>
        <td><%= link_to 'Destroy', todo_item, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-info pull-right" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
